<?xml version="1.0" encoding="utf-8"?>
<!--IIS Express调试下需要加这文件 不然上传不了大文件-->
<configuration>
	<system.webServer>
		<security>
			<requestFiltering>
				<!-- 文件请求最大限制50m -->
				<requestLimits maxAllowedContentLength="41943040" />
			</requestFiltering>
		</security>
	</system.webServer>
</configuration>